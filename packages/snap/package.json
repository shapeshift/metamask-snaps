{
  "name": "@shapeshift/metamask-snaps",
  "version": "0.7.0",
  "description": "A multi-chain Snap for MetaMask by ShapeShift.",
  "license": "MIT",
  "author": "pastaghost <pastaghost@2520.io> (http://www.github.com/pastaghost)",
  "homepage": "https://github.com/shapeshift/metamask-snaps/tree/main/packages/snap",
  "repository": {
    "type": "git",
    "url": "https://github.com/shapeshift/metamask-snaps.git"
  },
  "main": "build/index.js",
  "files": [
    "dist/",
    "images/",
    "snap.manifest.json"
  ],
  "scripts": {
    "build": "rm -rf dist; yarn run build:snap",
    "build:snap": "yarn build:snap:bundle && yarn build:snap:postprocess && yarn build:snap:eval",
    "build:snap:bundle": "mm-snap build --src src/index.ts --writeManifest false --manifest false --verboseErrors  --stripComments --eval false --transpilationMode localAndDeps",
    "build:snap:eval": "mm-snap eval -b dist/bundle.js --verboseErrors",
    "build:snap:postprocess": "node ./post-process.js && mm-snap manifest --fix",
    "clean": "rimraf dist/*",
    "dev": "mm-snap watch",
    "dev:sandbox": "concurrently --prefix \"{time}-[{name}]\" --names \"SNAP,SANDBOX\" -c \"bgBlue.bold,bgMagenta.bold\" \"yarn build && yarn link-packages\" \"cd examples/react-sandbox && yarn dev\"",
    "eval": "mm-snap eval -b dist/bundle.js",
    "postinstall": "patch-package",
    "lint": "eslint -c .eslintrc src --ext .ts,.tsx,.json --resolve-plugins-relative-to ../../",
    "lint:changelog": "yarn auto-changelog validate",
    "lint:fix": "yarn lint --fix && yarn lint:misc --write && yarn lint:package-json --write",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' --ignore-path .gitignore",
    "lint:package-json": "prettier-package-json ./package.json",
    "manifest": "mm-snap manifest --fix",
    "serve": "mm-snap serve",
    "test": "echo 'TODO'",
    "webpack": "webpack"
  },
  "dependencies": {
    "@metamask/detect-provider": "^1.2.0",
    "@metamask/key-tree": "^4.0.0",
    "@metamask/snaps-webpack-plugin": "^0.18.1",
    "@shapeshiftoss/caip": "^6.6.0",
    "@shapeshiftoss/hdwallet-core": "1.29.0",
    "@shapeshiftoss/hdwallet-native": "1.29.0",
    "@shapeshiftoss/logger": "^1.1.2"
  },
  "peerDependencies": {
    "@babel/core": "*",
    "@metamask/eslint-config": "*",
    "@shapeshiftoss/types": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "esling-plugin-prettier": "*",
    "eslint": "*",
    "eslint-plugin-jest": "*",
    "eslint-plugin-jsx-a11y": "*",
    "eslint-plugin-prettier": "*",
    "eslint-plugin-react": "*",
    "eslint-plugin-react-hooks": "*",
    "webpack": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@lavamoat/allow-scripts": "^2.0.3",
    "@metamask/auto-changelog": "^2.5.0",
    "@metamask/eslint-config": "^8.0.0",
    "@metamask/eslint-config-jest": "^8.0.0",
    "@metamask/eslint-config-nodejs": "^8.0.0",
    "@metamask/eslint-config-typescript": "^10.0.0",
    "@metamask/rollup-plugin-snaps": "^0.18.1",
    "@metamask/snap-types": "^0.19.1",
    "@metamask/snaps-browserify-plugin": "^0.18.1",
    "@metamask/snaps-cli": "^0.18.1",
    "@metamask/types": "^1.1.0",
    "@types/browserify": "^12.0.37",
    "@types/node": "^18.7.8",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "babel-loader": "^8.2.5",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "concurrently": "^7.3.0",
    "eslint": "^8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.8.7",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "mkdirp": "^1.0.4",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.7.1",
    "prettier-package-json": "^2.6.4",
    "prettier-plugin-packagejson": "^2.2.18",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "packageManager": "yarn@3.2.2"
}
