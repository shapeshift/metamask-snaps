{
  "name": "@shapeshiftoss/metamask-snaps",
  "version": "1.0.0-alpha.7",
  "description": "A multi-chain Snap for MetaMask by ShapeShift.",
  "license": "MIT",
  "author": "pastaghost <pastaghost@2520.io> (http://www.github.com/pastaghost)",
  "homepage": "https://github.com/shapeshift/metamask-snaps/tree/main/packages/snap",
  "repository": {
    "type": "git",
    "url": "https://github.com/shapeshift/metamask-snaps.git"
  },
  "main": "dist/index.js",
  "files": [
    "dist/",
    "src",
    "images/",
    "README.md",
    "snap.manifest.json"
  ],
  "scripts": {
    "build": "rm -rf dist; yarn run build:snap",
    "build:snap": "yarn build:snap:bundle && yarn build:snap:postprocess && yarn build:snap:eval",
    "build:snap:bundle": "mm-snap build --src src/index.ts --writeManifest false --manifest false --verboseErrors  --stripComments --eval false --transpilationMode localAndDeps",
    "build:snap:eval": "mm-snap eval -b dist/bundle.js --verboseErrors",
    "build:snap:postprocess": "node ./post-process.js && mm-snap manifest --fix",
    "clean": "rimraf dist/*",
    "dev": "mm-snap watch",
    "dev:sandbox": "concurrently --prefix \"{time}-[{name}]\" --names \"SNAP,SANDBOX\" -c \"bgBlue.bold,bgMagenta.bold\" \"yarn build && yarn serve\" \"cd ../sandbox && yarn dev\"",
    "dev:serve": "mm-snap watch --serve",
    "eval": "mm-snap eval -b dist/bundle.js --verboseErrors",
    "postinstall": "patch-package",
    "lint": "eslint -c .eslintrc src --ext .ts,.tsx,.json --resolve-plugins-relative-to ../../",
    "lint:changelog": "yarn auto-changelog validate",
    "lint:fix": "yarn lint --fix && yarn lint:misc --write && yarn lint:package-json --write",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' --ignore-path .gitignore",
    "lint:package-json": "prettier-package-json ./package.json",
    "manifest": "mm-snap manifest --fix",
    "serve": "mm-snap serve",
    "test": "echo 'No test suite defined for snap.'"
  },
  "dependencies": {
    "@ethersproject/providers": "^5.7.0",
    "@metamask/detect-provider": "^2.0.0",
    "@metamask/key-tree": "^9.0.0",
    "@metamask/snaps-types": "^1.0.1",
    "@metamask/snaps-ui": "^1.0.1",
    "@shapeshiftoss/caip": "^8.15.0",
    "@shapeshiftoss/hdwallet-core": "^1.34.0",
    "@shapeshiftoss/hdwallet-native": "^1.34.0",
    "@shapeshiftoss/logger": "^1.1.2",
    "@shapeshiftoss/metamask-snaps-types": "^1.0.0-alpha.1",
    "@shapeshiftoss/types": "^8.3.0",
    "@shapeshiftoss/unchained-client": "10.1.1"
  },
  "peerDependencies": {
    "@babel/core": "*",
    "@metamask/eslint-config": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "eslint": "*",
    "eslint-plugin-jest": "*",
    "eslint-plugin-jsx-a11y": "*",
    "eslint-plugin-prettier": "*",
    "eslint-plugin-react": "*",
    "eslint-plugin-react-hooks": "*",
    "webpack": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@lavamoat/allow-scripts": "^2.3.1",
    "@metamask/auto-changelog": "^3.1.0",
    "@metamask/eslint-config": "^11.1.0",
    "@metamask/eslint-config-jest": "^11.1.0",
    "@metamask/eslint-config-nodejs": "^11.1.0",
    "@metamask/eslint-config-typescript": "^11.1.0",
    "@metamask/snaps-cli": "0.32.2",
    "@metamask/types": "^1.1.0",
    "@types/browserify": "^12.0.37",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.8",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "babel-loader": "^8.2.5",
    "babelify": "^10.0.0",
    "concurrently": "^7.3.0",
    "eslint": "^8.23.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.8.7",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^29.0.3",
    "mkdirp": "^1.0.4",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.7.1",
    "prettier-package-json": "^2.6.4",
    "prettier-plugin-packagejson": "^2.2.18",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "packageManager": "yarn@3.2.2"
}
